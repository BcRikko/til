{
  "inputs": [
    {
      "type": "pickString",
      "id": "month",
      "description": "Select a yyyy-mm to filter posts",
      "options": [],
      "default": "2025-01"
    }
  ],
  "servers": {
    "filter-by-month": {
      "command": "find",
      "args": [
        "./src/pages/posts/${input:month}-*",
        "-type",
        "f"
      ],
    }
  }
}
